      parameter(nelxym=MAXNEL) ! Max number of elements in x-y plane

      common /array/ x(4,nelxym),y(4,nelxym)
     $             , bc(5,6,nelxym),curve(6,12,nelxym)

      common /arrai/ nlev,nel,ncurve,npscal,nelv

      common /arrac/ cbc,ccurve,ca
      character*3 cbc(6,nelxym)
      character*1 ccurve(12,nelxym)
      character*1 ca(nelxym)
