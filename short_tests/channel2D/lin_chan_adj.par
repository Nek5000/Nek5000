#
# nek parameter file
#
[GENERAL] 
#startFrom = restart.fld 
stopAt = numSteps #endTime
numSteps = 25000
perturbationModes = -1
userparam01 = 1

dt = -1.0e-02
timeStepper = bdf #char #steady
tOrder = 2

writeControl = timeStep #runTime
writeInterval = 90000

dealiasing = yes
filtering = no  
filterWeight = 1e-02

[PROBLEMTYPE]
perturbations = yes
solveBaseFlow = no

[PRESSURE]
preconditioner = semg
residualTol = 1e-08
residualProj = no

[VELOCITY]
residualTol = 1e-08
residualProj = no
density = 1
viscosity = -2000.0
