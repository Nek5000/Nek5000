[GENERAL]
stopat = endTime
endtime = 1.0
dt = 1e-3
timestepper = bdf3
writecontrol = runTime
writeinterval = 0.1
userparam01 = 1

[PROBLEMTYPE]
equation = lowMachNS
variableproperties = yes
dp0dt = yes

[MESH]
motion = user # elasticity

[PRESSURE]
residualtol = 1e-10
residualproj = no

[VELOCITY]
residualtol = 1e-12
residualproj = no

[TEMPERATURE]
solver = cvode
absolutetol = 1e-12

[CVODE]
relativetol = 1e-10
dtmax = 1e-3
