#
# nek parameter file
#
[GENERAL] 
#startFrom = restart.fld 
stopAt = nSteps
endTime = 50
numSteps = 1000

dt = 1e-03
timeStepper = bdf #bdf #char #steady
tOrder = 3
variableDt = no
maxCFL = 2.0

writeControl = timeStep #runTime
writeInterval = 0

dealiasing = yes
userParam01 = 1

[PROBLEMTYPE]
lowMachNumber = yes 
variableProperties = yes
dp0dt = yes

[MESH]
motion = yes 
meshVelocity = user

[PRESSURE]
residualTol = 1e-10
residualProj = no

[VELOCITY]
residualTol = 1e-12
residualProj = no

[TEMPERATURE]
solver = cvode
absoluteTol = 1e-12

[CVODE]
relativeTol = 1e-12
stiff = no
dtmax = 1e-2
