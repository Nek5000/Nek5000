#
# nek parameter file
#
[GENERAL] 
#startFrom = restart.fld 
stopAt = numSteps # or endTime
#endTime = 1.0
numSteps = 2.0e+3

dt = 1e-03
timeStepper = bdf #char #steady
tOrder = 3
variableDt = No
maxCFL = 0.4

writeControl = timeStep #runTime
writeInterval = 1000

dealiasing = no
filtering = no 
filterWeight = 0e-02

# high pass filter parameters
hpfAddModes = 0         # No of additional modes
hpfChi = -50           # filter strength

userParam01 = 1 # use in .usr uparam(01)

#[PROBLEMTYPE]
#stressFormulation = yes
#lowMachNumber = yes 
#dp0dt = yes
#variableProperties = yes

[PRESSURE]
preconditioner = semg
residualTol = 1e-08
residualProj = yes

[VELOCITY]
residualTol = 1e-8
residualProj = yes
density = 1.
viscosity = -1.0E+5
#writeToFieldFile = no 

