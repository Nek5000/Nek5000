#
# nek parameter file
#
[GENERAL] 
numSteps = 2000
dt = 1e-03
dealiasing = no
filtering = hpfrt        # explicit        
filterWeight = 50        # 0.05
filterCutOffRatio = 0.9  # fraction of unfiltered modes

[PROBLEMTYPE]
equation = incompNS

[PRESSURE]
residualTol = 1e-08
residualProj = yes

[VELOCITY]
residualTol = 1e-8
density = 1.
viscosity = -100000
