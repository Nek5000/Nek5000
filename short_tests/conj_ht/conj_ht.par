#
# nek parameter file
#
[GENERAL] 
numSteps = 1000
dt = 5e-02
writeInterval = 500

[PROBLEMTYPE]
equation = incompNS
variableProperties = yes

[PRESSURE]
residualTol = 1e-06
residualProj = no

[VELOCITY]
residualTol = 1e-07
residualProj = no
density = 1
viscosity = 0.5

[TEMPERATURE] # temperature with Hmholtz 
conjugateHeatTransfer = yes 
rhoCp = 1.0
conductivity = 0.5
residualTol = 1e-10

[SCALAR01] # temperature with CVODE
conjugateHeatTransfer = yes 
solver = cvode
absoluteTol = 1e-09

[CVODE]
relativeTol = 1e-04
stiff = no
dtmax = 1e-01
