#
# nek parameter file
#
[GENERAL] 
#startFrom = restart.fld 
stopAt = numSteps #endTime
numSteps = 1000

dt = 5e-02
timeStepper = bdf #char #steady
tOrder = 2

writeControl = timeStep #runTime
writeInterval = 500

dealiasing = yes
filtering = no  
filterWeight = 1e-02

[PROBLEMTYPE]
#stressFormulation = yes
variableProperties = yes

[PRESSURE]
preconditioner = semg
residualTol = 1e-06
residualProj = no

[VELOCITY]
residualTol = 1e-08
residualProj = no
density = 1
viscosity = 0.5

[TEMPERATURE] # temperature with Hmholtz 
conjugateHeatTransfer = yes 
rhoCp = 1.0
conductivity = 0.5
residualTol = 1e-08

[SCALAR01] # temperature with CVODE
conjugateHeatTransfer = yes 
solver = cvode
absoluteTol = 1e-08

[CVODE]
relativeTol = 1e-04
stiff = no
dtmax = 1e-01
