#
# nek parameter file
#
[GENERAL] 
polynomialOrder = 7
PnPnFormulation = yes
minNumProcesses = 1
maxNumProcesses = 4

dealiasing = no
extrapolation = OIFS
#startFrom = restart.fld 
numSteps = 1000
dt = 1e-04
timeStepper = bdf3 #bdf2
writeInterval = 0

[PROBLEMTYPE]
equation = incompNS

[PRESSURE]
preconditioner = semg_amg
residualTol = 1e-08
residualProj = no

[VELOCITY]
residualTol = 1e-12
residualProj = no
density = 1
viscosity = -10 
